generator client {
  provider = "prisma-client-js"
}

generator pothos {
  provider = "prisma-pothos-types"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db"
}

model File {
  id       Int    @id @default(autoincrement())
  name     String
  type     String
  blobName String
  url      String
}
